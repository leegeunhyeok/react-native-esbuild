    "@react-native-esbuild/plugins": "workspace:*",
    "@react-native-esbuild/shared": "workspace:*",
    "@react-native-esbuild/transformer": "workspace:*",
    "chokidar": "^3.5.3",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.19.5",
    "esbuild-dependency-graph": "^0.3.0",
    "invariant": "^2.2.4",
    "ora": "^5.4.1"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@faker-js/faker": "^8.1.0",
    "@swc/core": "^1.3.95"
  }
