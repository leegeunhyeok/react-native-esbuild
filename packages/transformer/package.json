    "@react-native-esbuild/shared": "workspace:*",
    "@swc/core": "^1.10.4",
    "@swc/helpers": "^0.5.15",
    "esbuild-dependency-graph": "^0.3.0",
    "hermes-parser": "^0.23.1",
    "md5": "^2.3.0",
    "sucrase": "^3.35.0",
    "swc-plugin-coverage-instrument": "^0.0.25",
    "swc_mut_cjs_exports": "^8.0.1"
